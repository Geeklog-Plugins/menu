    <form method="post" action="{form_action}" id="clonemenu">
    <div>
      <div class="required">
        <label for="menuname">{LANG_MENU01[clone_menu_label]}:</label>
        <input class="fValidate['required']" type="text" id="menuname" name="menuname" size="48" value="{menuname}"{xhtml}>

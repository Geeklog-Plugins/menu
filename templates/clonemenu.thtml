      <input type="submit" id="execute" name="execute" value="{LANG_MENU01[save]}"{xhtml}>&nbsp;&nbsp;
      <input type="submit" id="cancel" name="cancel" value="{LANG_MENU01[cancel]}"{xhtml}>
    </div>
  </div>
  </form>
  </div>

